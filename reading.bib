@Article{rand-tarp03,
  author = 	 {J. Rand and F. Tarp},
  title = 	 {Business Cycles in Developing Countries},
  journal = 	 {World Development},
  year = 	 2003,
  volume =	 30,
  number =	 12,
  pages =	 {2071--2089},
  annote =	 {Uses methods from the study of U.S. business cycles
                  to come up with stylized facts regarding business
                  cycles in developing countries.  Most striking
                  finding is that business cycle frequencies for
                  developing countries is much higher than for U.S.,
                  with an average period of 10.6 quarters, vs. about
                  32 for the U.S.  India is supposed to have a
                  business cycle period of only about 8 quarters!}
}

