@article{kurosaki-fafchamps02,
  author =	 "Kurosaki, Takashi and Fafchamps, Marcel",
  title =	 "{Insurance market efficiency and crop choices in Pakistan}",
  journal =	 "Journal of Development Economics",
  year =	 "2002",
  volume =	 "67",
  number =	 "2",
  pages =	 "419--453",
  note =	 "April",
  read =	 "nH",
  library =	 "gi
                  ma  HC59.7 .J6
                  nr  HC59.7 .J6",
  refdin =	 "",
  keywords =	 "",
  bib =		 "",
  notes =	 "",
  date =	 "19020420",
  abstract =	 "
*'This paper tests the efficiency of insurance markets in the
 Pakistan Punjab by examining how crop choices are affected by the
 presence of price and yield risk. Reduced-form and structural
 models of crop choices are estimated. Although the paper cannot
 reject the hypothesis that village members efficiently share risk
 among themselves, production choices are shown to depend on risk.
 Existing risk sharing and self-insurance mechanisms thus
 imperfectly protect Punjab farmers against village-level shocks.
 Results also indicate that households respond to consumption price
 risk, thereby suggesting that empirical and theoretical work on
 risk should avoid putting an exclusive emphasis on yield and
 output price risk.'"
}

