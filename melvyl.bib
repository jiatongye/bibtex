@article{deaton02,
  author =	 "Deaton, Angus",
  title =	 "{Policy implications of the gradient of health and wealth}",
  journal =	 "Health Affairs",
  year =	 "2002",
  volume =	 "21",
  number =	 "2",
  pages =	 "13--30",
  note =	 "March/April",
  read =	 "nH",
  library =	 "pu  RA394.A1 H4",
  refdin =	 "",
  keywords =	 "",
  bib =		 "",
  notes =	 "",
  date =	 "19020331",
  abstract =	 "
*'Men in the US with family incomes in the top 5% of the
 distribution in 1980 had about 25% longer to live than did those in the
 bottom 5%. Proportional increases in income are associated with
 equal proportional decreases in mortality throughout the income
 distribution. This article discusses possible reasons for this
 gradient and asks whether it calls for the redistribution of
 income in the interest of public health. It is argued that the
 existence of the gradient strengthens the case for income
 redistribution in favor of the poor but that targeting health inequalities
 would not be sound policy.'"
}

@article{case-paxson02,
  author =	 "Case, Anne and Paxson, Christina",
  title =	 "{Parental behavior and child health}",
  journal =	 "Health Affairs",
  year =	 "2002",
  volume =	 "21",
  number =	 "2",
  pages =	 "164--178",
  note =	 "March/April",
  read =	 "nH",
  library =	 "pu  RA394.A1 H4",
  refdin =	 "",
  keywords =	 "",
  bib =		 "",
  notes =	 "",
  date =	 "19020331",
  abstract =	 "
*'This paper documents the ways in which parental behavior and
 socioeconomic status affect children's health. Parental behavior
 in both the prenatal period and childhood are examined, and
 evidence is presented on the correlation of this behavior with income
 and parents' socioeconomic status, and on the ways in which
 parents' actions affect children's health. It is concluded that
 while health insurance coverage and advances in medical treatment
 may be important determinants of children's health, they cannot
 be the only pillars: Protecting children's health also calls for
 a broader set of policies that target parents' health-related
 behavior.'"
}

