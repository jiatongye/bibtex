@article{case02,
  author =	 "Case, Mary Anne",
  title =	 "{Reflections on constitutionalizing women's equality}",
  journal =	 "California Law Review",
  year =	 "2002",
  volume =	 "90",
  number =	 "3",
  pages =	 "765--790",
  note =	 "May",
